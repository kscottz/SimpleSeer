<div class="row">
  <div style="position: fixed;">
    <div id="icon-menu">
      <div class="icon-item">
      </div>
    </div>
    <!--second tier menu panel -->
    <div id="second-tier-menu">
      <div class="second-tier-item">
        <div id="second-tier-header">
          <span class="title">Control Panel</span>
          <img id="minimize-control-panel" src="img/arrows-minimize.png" />
        </div>
      </div>
      <div id="filter_form"></div>
        <div id="second-tier-footer">
          <p style="clear:both;">Viewing <span id="count_viewing">{{count_viewing}}</span> of <span id="count_total">{{count_total}}</span> frames.</p>
        </div>
    </div>
  </div>
</div>

  <div class="span9 offset3">
  
       <!--MAIN TABS -->
      <div id="views">
        <div id="views-tabs">
          <div id="image-tab">Image View</div>
          <div id="data-tab" class="unselected">Data View</div>
        </div>
        <div id="views-contain">
          <div id="views-controls">
            <div id="control-images">
              <img id="list-view" src="img/icon-list-view.png"><img id="grid-view" class="list-unselected" src="img/icon-grid-view.png">
            </div>
            <div id="control-copy">
              <span class="selection-tog"><a href="#">Select All / </a><a href="#"><span class="copy-off">None</span></a></span> <span class="selection-tog"><a href="#">Edit</a> / <a href="#"><span class="copy-off">Edit All</span></a></span>
              <span>List:</span>
              <select id="sortCombo">
              {{#each sortComboVals}}
                <option value="{{this.name}},1">{{this.label}} ascending</option>
                <option value="{{this.name}},-1">{{this.label}} descending</option>
              {{/each}}
              </select>
            </div>
          </div>
          <div class="data-view-item">

            <div id="frame_holder"></div>
    
          </div><!--END GRID IMAGE VIEW -->
            
            
            <div id="data-views-contain">
              <div id="data-view">
                <h1>data view!</h1>
              </div> <!-- END DATA VIEW ITEM -->
            </div><!-- END DATA VIEW ITEM -->
          </div>
    
    
    
    
    
  </div>
</div>
<div id="loading_message" class="alert alert-info span11">
  Loading <span id="next_page_size">20</span> more frames...
</div>
